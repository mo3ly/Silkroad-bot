<Application x:Class="SRO_INGAME.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SRO_INGAME"
             StartupUri="View\Main\Launcher.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/Cursors.xaml"/>
                <ResourceDictionary Source="View/Styles/Buttons.xaml"/>
                <ResourceDictionary Source="View/Styles/TextControl.xaml"/>
                <ResourceDictionary Source="View/Styles/ComboBox.xaml"/>
                <ResourceDictionary Source="View/Styles/CheckBox.xaml"/>
                <ResourceDictionary Source="View/Styles/RadioButton.xaml"/>
                <!--<ResourceDictionary Source="View/Styles/ListView.xaml"/>-->
                <ResourceDictionary Source="View/Styles/Scrollbar.xaml"/>
                <ResourceDictionary Source="View/Styles/Slider.xaml"/>
                <ResourceDictionary Source="View/Styles/DataGrid.xaml"/>
                <ResourceDictionary Source="View/Styles/ToolTip.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
